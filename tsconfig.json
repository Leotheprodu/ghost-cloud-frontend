{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": "./src",
    "paths": {
      "@/*": ["*"],
      "@styles/*": ["styles/*"],
      "@chore/*": ["modules/chore/*"],
      "@modules/*": ["components/*"],
      "@stores/*": ["stores/*"],
      "@icons/*": ["icons/*"]
    }
  }
}
